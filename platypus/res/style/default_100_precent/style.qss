/*游戏界面标题栏按钮 %1 button圆角 */

QWidget#widget_caption{
border-image: url(:/1kplatypus/image/default_100_precent/caption_bg.png);
}

QWidget#platypusClass{
border:1px solid #303031;
}

QStackedWidget {
background-color: rgb(60, 60, 64);
}

/*关闭按钮*/
QPushButton#btn_close{  
border-image: url(:/1kplatypus/image/default_100_precent/close.png);
border:none;
}  

QPushButton#btn_close:hover{  
border-image: url(:/1kplatypus/image/default_100_precent/close_hover.png);
border:none;
}  

QPushButton#btn_close:pressed{  
border-image: url(:/1kplatypus/image/default_100_precent/close_pressed.png);
border:none;
} 

