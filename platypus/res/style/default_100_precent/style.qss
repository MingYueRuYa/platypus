QPushButton#btn_refresh {
border-image: url(:/1kplatypus/image/default_100_precent/refresh.png);
border-width:0px;
}

QPushButton#btn_refresh:hover {
border-image: url(:/1kplatypus/image/default_100_precent/refresh_hover.png);
border-radius:%1px;
}

QPushButton#btn_refresh:pressed {
border-image: url(:/1kplatypus/image/default_100_precent/refresh_pressed.png);
border-radius:%1px;
}

QPushButton#btn_refresh:disabled {
border-image: url(:/1kplatypus/image/default_100_precent/refresh_disable.png);
border-radius:%1px;
}

QToolButton#btn_gohome {
border-radius:%1px;
font-size:9pt;
font-family:Microsoft YaHei;
background:transparent;
color:#AAAAAA;
background-color:rgba(255,255,255,0.01);
padding-left:5px;
padding-right:5px;
}

QToolButton#btn_gohome:hover {
border-radius:%1px;
background-color:rgba(255,255,255,0.1);
padding-left:5px;
padding-right:5px;
color:#FFFFFF;
}

QToolButton#btn_gohome:pressed {
border-radius:%1px;
background:rgba(255,255,255,13);
color:rgba(204, 204, 204, 1);
border-radius:4px;
border:none;
}


